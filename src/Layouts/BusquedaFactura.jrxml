	<parameter name="Fecha" class="java.lang.String"/>
	<queryString>
		<![CDATA[select a.idFactura,a.fechaEmision,b.Cliente,b.Vendedor,c.Nombre,a.cant,c.precio,a.total,d.descripcion,
(select SUM(total)total from  facturaproducto  where idFactura=$P{Fecha})neto,
(select round(SUM(total)*0.04,2)  from  facturaproducto  where idFactura=$P{Fecha})ISV,
(select SUM(total)+(select round(SUM(total)*0.04,2)  from  facturaproducto  where idFactura=(Select idFactura from facturaproducto where idFactura=$P{Fecha} limit 1))  from  facturaproducto  where idFactura=(Select idFactura from facturaproducto where idFactura=$P{Fecha} limit 1))Pagar
from   `facturaproducto` as a 
join `factura` as b on a.idFactura=b.idFactura
join producto as c on a.idProducto=c.idProducto
join formapago as d on b.formapago=d.idFormaPago
where a.idFactura=$P{Fecha}]]>
	</queryString>
